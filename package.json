    "@ngrx/schematics": "^14.3.3",
    "@types/jasmine": "~4.0.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
